,\n        { headers: { \"x-auth-token\": token } }\n      );\n      if (tokenRes.data) {\n        dispatch({\n          type: 'USERDATA',\n          token1: token,\n          user1: userss\n        });\n      }\n    };\n    checkLoggedIn();\n  }, []);\n\n  const alertDisappear1 = () => {\n    dispatch({\n      type: 'ALERT1',\n      alert1: false\n    });\n  }\n  const alertDisappear2 = () => {\n    dispatch({\n      type: 'ALERT2',\n      alert2: false\n    });\n  }\n\n\n  return (\n\n    <Router>\n\n      <div>\n        {alert1 ? <div className='alertBlack1'>\n          <div className=\"alertoff\">\n            You didn't select your Address or you are more than 30km far from Lieferando.\n            <hr />\n            <button className='alertoffBtn1' onClick={alertDisappear1}>OK</button>\n          </div>\n        </div>\n          : ''}\n        {alert2 ? <div className='alertBlack1'>\n          <div className=\"alertoff\">\n            Please select your address\n            <hr />\n            <button className='alertoffBtn1' onClick={alertDisappear2}>OK</button>\n          </div>\n        </div>\n          : ''}\n        <Switch>\n          <Route exact path=\"/\" component={Dashboard} />\n          {/* <Route exact path=\"/login\" component={Login} />\n          <Route exact path=\"/register\" component={Register} /> */}\n          <Route exact path=\"/blog\" component={Blog} />\n          <Route exact path={`/register`} render={props => <Register {...props} />} />\n          <Route exact path={`/login`} render={props => <Login {...props} />} />\n          <Route exact path={`/auth/activate/:token`} render={props => <Active {...props} />} />\n          <Route exact path=\"/profile\" component={Profile} />\n          <Route exact path=\"/orders\" component={Orders} />\n          <Route exact path=\"/paidorders\" component={PaidOrders} />\n          <Route path=\"/blog/show/\" component={BlogDetail} />\n          <Route path=\"/menu\" component={Products} />\n        </Switch>\n      </div>\n    </Router>\n\n  );\n}\n\nexport default App;\n\n\n\n","/home/ismail/Desktop/Liferando/client/src/components/root/Dashboard.js",[],"/home/ismail/Desktop/Liferando/client/src/components/root/Orders.js",["130","131"],"/home/ismail/Desktop/Liferando/client/src/components/root/Blog.js",[],"/home/ismail/Desktop/Liferando/client/src/components/root/Login.js",["132","133"],"\n// export default Login\nimport React, { useState } from \"react\";\nimport { useHistory } from \"react-router-dom\";\nimport Axios from \"axios\";\nimport ErrorNotice from \"../misc/ErrorNotice\";\nimport { useStateValue } from \"../../StateProvider\";\n\nexport default function Login() {\n\n\n    const [email, setEmail] = useState();\n    const [password, setPassword] = useState();\n    const [error, setError] = useState();\n    const [username, setUsername] = useState();\n\n    const [{ userData }, dispatch] = useStateValue()\n\n    const history = useHistory();\n\n    const submit = async (e) => {\n        e.preventDefault();\n        try {\n            const loginUser = { email, password, username };\n            const loginRes = await Axios.post(\n                `http://localhost:${process.env.PORT}/auth/login`,\n                loginUser\n            )\n            dispatch({\n                type: 'USERDATA',\n                token1: loginRes.data.token,\n                user1: loginRes.data.user\n            });\n            localStorage.setItem(\"auth-token\", loginRes.data.token);\n            localStorage.setItem(\"user-data\", JSON.stringify(loginRes.data.user));\n            if (email == 'admin@example.com') {\n                window.location.href = `http://localhost:${process.env.PORT}}/admin`;\n            } else {\n                history.push(\"/\");\n            }\n        } catch (err) {\n            err.response.data.msg && setError(err.response.data.msg);\n        }\n    };\n    return (\n\n        <div className='min-h-screen bg-gray-100 tex